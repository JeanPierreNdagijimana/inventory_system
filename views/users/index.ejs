<div>
  <h1>Users</h1>
  <%- include('../partials/messages'); -%>
    <table class="table table-hover table-striped">
      <thead>
        <tr>
          <th>#</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
          <th>Username</th>
          <th>Created At</th>
        </tr>
      </thead>

      <tbody>
        <% users.forEach((user)=> {%>
          <tr>
            <td>
              <%= user.id %>
            </td>
            <td>
              <%= user.first_name %>
            </td>
            <td>
              <%= user.last_name %>
            </td>
            <td>
              <%= user.email %>
            </td>
            <td>
              <%= user.username %>
            </td>
            <td>
              <%= user.createdAt.toLocaleString() %>
            </td>
          </tr>
          <% }) %>
      </tbody>
    </table>
</div>